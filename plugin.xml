<?xml version='1.0' encoding='utf-8'?>
<plugin id="gid.com.gidvisionlib.plugin" version="0.0.1" 
    xmlns="http://apache.org/cordova/ns/plugins/1.0" 
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>GidLibVision</name>
    <js-module name="GidLibVision" src="www/GidLibVision.js">
        <clobbers target="cordova.plugins.GidLibVision" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="GidLibVision">
                <param name="android-package" value="gid.com.gidvisionlib.plugin.GidLibVision" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/GidLibVision.java" target-dir="src/gid/com/gidvisionlib/plugin/GidLibVision" />
        <framework custom="true" src="src/android/GidLibVisionConfig.gradle" type="gradleReference" />
        <resource-file src="src/android/libs/gidvisionlib-debug.aar" target="libs/gidvisionlib-debug.aar" />
    </platform>
</plugin>